{
    "name": "Python Boston Dynamics Environment",
    "dockerComposeFile": "../docker-compose.yml",
    "service": "ps9-boston-dynamic-mobile-cv-testing-systems",
    "workspaceFolder": "/workspaces/PS9-Boston-Dynamic-Mobile-CV-Testing-Systems",
    "settings": {
        "python.defaultInterpreterPath": "/workspaces/PS9-Boston-Dynamic-Mobile-CV-Testing-Systems/.venv/bin/python"
    },
    "extensions": [
        "ms-python.python",
        "ms-python.vscode-pylance",
        "yzhang.markdown-all-in-one",
        "bierner.markdown-mermaid",
        "cweijan.vscode-database-client2"
    ],
    "remoteUser": "root",
    "postCreateCommand": "git submodule update --init --recursive && python -m venv .venv && .venv/bin/pip install --upgrade pip && .venv/bin/pip install -r requirements.txt && mkdir -p src/third_party/dinov2/checkpoints && curl -L https://dl.fbaipublicfiles.com/dinov2/dinov2_vits14/dinov2_vits14_pretrain.pth -o src/third_party/dinov2/checkpoints/dinov2_vits14.pth",
    "customizations": {
        "vscode": {
            "settings": {
                "python.defaultInterpreterPath": "/workspaces/PS9-Boston-Dynamic-Mobile-CV-Testing-Systems/.venv/bin/python",
                "python.pythonPath": "/workspaces/PS9-Boston-Dynamic-Mobile-CV-Testing-Systems/.venv/bin/python"
            }
        }
    },
    "forwardPorts": [9000, 9001],
    "portsAttributes": {
        "9000": {
            "label": "MinIO API",
            "onAutoForward": "notify"
        },
        "9001": {
            "label": "MinIO Console",
            "onAutoForward": "openBrowser"
        }
    }
}